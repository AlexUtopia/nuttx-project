+/****************************************************************************
+ * Public Functions
+ ****************************************************************************/
+
+/****************************************************************************
+ * Name: board_app_initialize
+ *
+ * Description:
+ *   Perform architecture specific initialization for NSH.
+ *
+ *   CONFIG_NSH_ARCHINIT=y :
+ *     Called from the NSH library
+ *
+ *   CONFIG_BOARD_INITIALIZE=y, CONFIG_NSH_LIBRARY=y, &&
+ *   CONFIG_NSH_ARCHINIT=n:
+ *     Called from board_initialize().
+ *
+ ****************************************************************************/
+#define PRINT(fmt, ...)    lowsyslog(0, fmt, ##__VA_ARGS__)
+int board_app_initialize(void)
+{
+  struct spi_dev_s *spi;
+  FAR struct mtd_dev_s *mtd;
+  int ret;
+
+  PRINT("SPI init :");
+  spi = up_spiinitialize(0);
+  if (spi == NULL)
+  {
+    PRINT("FAIL\n");
+    return -1;
+  }
+  PRINT("OK\n");
+
+  PRINT("M25P init : ");
+  mtd = m25p_initialize(spi);
+  if (!mtd)
+  {
+    PRINT("FAIL\n");
+    return -1;
+  }
+  PRINT("OK\n");
+#if 0
+  PRINT("S512 init : ");
+  mtd = /*s512_initialize*/mtd_rwb_initialize(mtd);
+  if (!mtd)
+  {
+	PRINT("FAIL\n");
+	return -1;
+  }
+  PRINT("OK\n");
+#endif
+  PRINT("Block device init : ");
+  ret = ftl_initialize(0, mtd);
+  if (ret < 0)
+  {
+    PRINT("FAIL\n");
+    return -1;
+  }
+  PRINT("OK\n");
+#if 0
+  PRINT("Char device init : ");
+  ret = bchdev_register("/dev/mtdblock0", "/dev/mtd0", false);
+  if (ret < 0)
+    {
+      PRINT("FAIL\n");
+      return -1;
+    }
+  PRINT("OK\n");
+#endif
+
+  return 0;
+}
